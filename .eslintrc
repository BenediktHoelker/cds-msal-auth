{
  "env": {
    "node": true,
    "browser": true,
    "jasmine": true,
    "protractor": true,
    "jquery": true,
    "es6": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-underscore-dangle": 0,
    "func-names": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2020
  }
}
